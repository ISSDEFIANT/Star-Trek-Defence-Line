						ship.GetComponent<Castle> ().AI = Slot1.GetComponent<SaveInfo> ().AI [0];
						ship.GetComponent<Castle> ().FreandAI = Slot1.GetComponent<SaveInfo> ().FriendAI [0];

						ship.transform.position = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].position;
