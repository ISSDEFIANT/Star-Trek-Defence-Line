	// Update is called once per frame
	void Update () {
		if (load) {
			if (SelectSlot1) {
				if (!LoadLevel) {
					Application.LoadLevel (Slot1.GetComponent<SaveInfo> ().LevelName);
					LoadLevel = true;
				}
				if (Slot1.GetComponent<SaveInfo> ().SaveObjects.Count > 0) {
					if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FedBS" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FedSS" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Defiant" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Nova" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Saber" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Akira" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Intrepid" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Steamrunner" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Luna" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Promentheus" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Nebula" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Galaxy" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Sovereign" || Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Excalibur") {
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FedBS") {
							LoadingObject = (GameObject)Instantiate (FBS, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FedSS") {
							LoadingObject = (GameObject)Instantiate (FTS, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Defiant") {
							LoadingObject = (GameObject)Instantiate (FS1, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Nova") {
							LoadingObject = (GameObject)Instantiate (FS2, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Saber") {
							LoadingObject = (GameObject)Instantiate (FS3, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Akira") {
							LoadingObject = (GameObject)Instantiate (FS4, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Intrepid") {
							LoadingObject = (GameObject)Instantiate (FS5, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Steamrunner") {
							LoadingObject = (GameObject)Instantiate (FS6, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Luna") {
							LoadingObject = (GameObject)Instantiate (FS7, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Promentheus") {
							LoadingObject = (GameObject)Instantiate (FS8, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Nebula") {
							LoadingObject = (GameObject)Instantiate (FS9, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Galaxy") {
							LoadingObject = (GameObject)Instantiate (FS10, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Sovereign") {
							LoadingObject = (GameObject)Instantiate (FS11, gameObject.transform.position, gameObject.transform.rotation);
						}
						if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "Excalibur") {
							LoadingObject = (GameObject)Instantiate (FS12, gameObject.transform.position, gameObject.transform.rotation);
						}
						GameObject ship;
						ship = LoadingObject;
						ship.GetComponent<Health> ().curHealth = Slot1.GetComponent<SaveInfo> ().ObjectsHealth [0];
						ship.GetComponent<Health> ().CurСилаПоля = Slot1.GetComponent<SaveInfo> ().ObjectsShilds [0];
						//ship.GetComponent<Health> ().curGeneratorHealth = Slot1.GetComponent<SaveInfo> ().ObjectsGenerator [0];
						//ship.GetComponent<Health> ().curWeaponHealth = Slot1.GetComponent<SaveInfo> ().ObjectsWeapon [0];
						//ship.GetComponent<Health> ().curEngenHealth = Slot1.GetComponent<SaveInfo> ().ObjectsEngings [0];

						ship.GetComponent<Stats> ().AI = Slot1.GetComponent<SaveInfo> ().AI [0];
						ship.GetComponent<Stats> ().FreandAI = Slot1.GetComponent<SaveInfo> ().FriendAI [0];

						ship.GetComponent<ActiveState> ().CanAttack = Slot1.GetComponent<SaveInfo> ().CanAttack [0];

						ship.GetComponent<ActiveState> ().Build = Slot1.GetComponent<SaveInfo> ().Build [0];
						ship.GetComponent<ActiveState> ().isFlag = Slot1.GetComponent<SaveInfo> ().IsFlag [0];

						ship.GetComponent<ActiveState> ().Agrass = Slot1.GetComponent<SaveInfo> ().Agrass [0];
						ship.GetComponent<ActiveState> ().Protact = Slot1.GetComponent<SaveInfo> ().Protact [0];
						ship.GetComponent<ActiveState> ().Idle = Slot1.GetComponent<SaveInfo> ().Idle [0];

						ship.transform.position = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].position;
						ship.transform.rotation = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].rotation;

						Slot1.GetComponent<SaveInfo> ().SaveObjects.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsHealth.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsShilds.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsGenerator.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsWeapon.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsEngings.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectTransform.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().AI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().FriendAI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().CanAttack.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Build.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().IsFlag.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Agrass.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Protact.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Idle.RemoveAt (0);
					}
					if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FBase") {
						GameObject ship;
						ship = (GameObject)Instantiate (FBase, gameObject.transform.position, gameObject.transform.rotation);

						ship.GetComponent<Health> ().curHealth = Slot1.GetComponent<SaveInfo> ().ObjectsHealth [0];
						ship.GetComponent<Health> ().CurСилаПоля = Slot1.GetComponent<SaveInfo> ().ObjectsShilds [0];
						//ship.GetComponent<Health> ().curGeneratorHealth = Slot1.GetComponent<SaveInfo> ().ObjectsGenerator [0];
						//ship.GetComponent<Health> ().curWeaponHealth = Slot1.GetComponent<SaveInfo> ().ObjectsWeapon [0];
						//ship.GetComponent<Health> ().curEngenHealth = Slot1.GetComponent<SaveInfo> ().ObjectsEngings [0];

						ship.GetComponent<Castle> ().AI = Slot1.GetComponent<SaveInfo> ().AI [0];
						ship.GetComponent<Castle> ().FreandAI = Slot1.GetComponent<SaveInfo> ().FriendAI [0];

						ship.transform.position = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].position;
						ship.transform.rotation = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].rotation;

						Slot1.GetComponent<SaveInfo> ().SaveObjects.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsHealth.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsShilds.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsGenerator.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsWeapon.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsEngings.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectTransform.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().AI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().FriendAI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().CanAttack.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Build.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().IsFlag.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Agrass.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Protact.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Idle.RemoveAt (0);
					}
					if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FMining") {
						GameObject ship;
						ship = (GameObject)Instantiate (FMine, gameObject.transform.position, gameObject.transform.rotation);

						ship.GetComponent<Health> ().curHealth = Slot1.GetComponent<SaveInfo> ().ObjectsHealth [0];
						ship.GetComponent<Health> ().CurСилаПоля = Slot1.GetComponent<SaveInfo> ().ObjectsShilds [0];
						//ship.GetComponent<Health> ().curGeneratorHealth = Slot1.GetComponent<SaveInfo> ().ObjectsGenerator [0];
						//ship.GetComponent<Health> ().curWeaponHealth = Slot1.GetComponent<SaveInfo> ().ObjectsWeapon [0];
						//ship.GetComponent<Health> ().curEngenHealth = Slot1.GetComponent<SaveInfo> ().ObjectsEngings [0];

						ship.GetComponent<Castle> ().AI = Slot1.GetComponent<SaveInfo> ().AI [0];
						ship.GetComponent<Castle> ().FreandAI = Slot1.GetComponent<SaveInfo> ().FriendAI [0];

						ship.transform.position = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].position;
						ship.transform.rotation = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].rotation;

						Slot1.GetComponent<SaveInfo> ().SaveObjects.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsHealth.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsShilds.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsGenerator.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsWeapon.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsEngings.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectTransform.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().AI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().FriendAI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().CanAttack.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Build.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().IsFlag.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Agrass.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Protact.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Idle.RemoveAt (0);
					}
					if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FShop") {
						GameObject ship;
						ship = (GameObject)Instantiate (FShop, gameObject.transform.position, gameObject.transform.rotation);

						ship.GetComponent<Health> ().curHealth = Slot1.GetComponent<SaveInfo> ().ObjectsHealth [0];
						ship.GetComponent<Health> ().CurСилаПоля = Slot1.GetComponent<SaveInfo> ().ObjectsShilds [0];
						//ship.GetComponent<Health> ().curGeneratorHealth = Slot1.GetComponent<SaveInfo> ().ObjectsGenerator [0];
						//ship.GetComponent<Health> ().curWeaponHealth = Slot1.GetComponent<SaveInfo> ().ObjectsWeapon [0];
						//ship.GetComponent<Health> ().curEngenHealth = Slot1.GetComponent<SaveInfo> ().ObjectsEngings [0];

						ship.GetComponent<Castle> ().AI = Slot1.GetComponent<SaveInfo> ().AI [0];
						ship.GetComponent<Castle> ().FreandAI = Slot1.GetComponent<SaveInfo> ().FriendAI [0];

						ship.transform.position = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].position;
						ship.transform.rotation = Slot1.GetComponent<SaveInfo> ().ObjectTransform [0].rotation;

						Slot1.GetComponent<SaveInfo> ().SaveObjects.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsHealth.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsShilds.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsGenerator.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsWeapon.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectsEngings.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().ObjectTransform.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().AI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().FriendAI.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().CanAttack.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Build.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().IsFlag.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Agrass.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Protact.RemoveAt (0);
						Slot1.GetComponent<SaveInfo> ().Idle.RemoveAt (0);
					}
					if (Slot1.GetComponent<SaveInfo> ().SaveObjects [0] == "FMagic") {
						GameObject ship;
						ship = (GameObject)Instantiate (FMagic, gameObject.transform.position, gameObject.transform.rotation);

						ship.GetComponent<Health> ().curHealth = Slot1.GetComponent<SaveInfo> ().ObjectsHealth [0];
						ship.GetComponent<Health> ().CurСилаПоля = Slot1.GetComponent<SaveInfo> ().ObjectsShilds [0];
						//ship.GetComponent<Health> ().curGeneratorHealth = Slot1.GetComponent<SaveInfo> ().ObjectsGenerator [0];
						//ship.GetComponent<Health> ().curWeaponHealth = Slot1.GetComponent<SaveInfo> ().ObjectsWeapon [0];
