	public void Save(){
		if (SelectSlot1) {
			Slot1.GetComponent<SaveInfo> ().SaveName = curSaveName;

			Slot1.GetComponent<SaveInfo> ().SaveObjects = SaveObjects;

			Slot1.GetComponent<SaveInfo> ().ObjectsHealth = ObjectsHealth;
			Slot1.GetComponent<SaveInfo> ().ObjectsShilds = ObjectsShilds;
			Slot1.GetComponent<SaveInfo> ().ObjectsGenerator = ObjectsGenerator;
			Slot1.GetComponent<SaveInfo> ().ObjectsWeapon = ObjectsWeapon;
			Slot1.GetComponent<SaveInfo> ().ObjectsEngings = ObjectsEngings;

			Slot1.GetComponent<SaveInfo> ().AI = AI;
			Slot1.GetComponent<SaveInfo> ().FriendAI = FriendAI;

			Slot1.GetComponent<SaveInfo> ().CanAttack = CanAttack;

			Slot1.GetComponent<SaveInfo> ().Build = Build;
			Slot1.GetComponent<SaveInfo> ().IsFlag = IsFlag;

			Slot1.GetComponent<SaveInfo> ().Agrass = Agrass;
			Slot1.GetComponent<SaveInfo> ().Protact = Protact;
			Slot1.GetComponent<SaveInfo> ().Idle = Idle;

			Slot1.GetComponent<SaveInfo> ().ObjectTransform = SaveObjectsTransforms;

			Slot1.GetComponent<SaveInfo> ().LevelName = Application.loadedLevelName; 
		}
		if (SelectSlot2) {
			Slot2.GetComponent<SaveInfo> ().SaveName = curSaveName;

			Slot2.GetComponent<SaveInfo> ().SaveObjects = SaveObjects;

